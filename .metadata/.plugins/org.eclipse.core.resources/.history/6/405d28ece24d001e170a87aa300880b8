package data;

public class CharType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
		
		
		
		
		
		
		
		//유니코드 - 16bit - 2Byte
		char uniCode1 = '한'; //한 문자 - 홑따움표
		System.out.println(uniCode1);
		
		char uniCode2 = '\uD55C';
		System.out.println(uniCode2);
	}

}
