package forexample;

public class Practice230908 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int num = 1; num <=10; num++) {
			System.out.print(num + " ");
		}
		System.out.println();

		int i = 0;
		int sum = 0;
		for(i=1, sum=0; i<=10; i++) {
			sum += i;
		}
		System.out.println("합계: " + sum);
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
